            id="us.coastalhacking.semiotics.model.control.presentation.ControlEditorID"
            name="%_UI_ControlEditor_label"
            icon="icons/full/obj16/ControlModelFile.gif"
            extensions="control"
            class="us.coastalhacking.semiotics.model.control.presentation.ControlEditor"
            contributorClass="us.coastalhacking.semiotics.model.control.presentation.ControlActionBarContributor">
      </editor>
