      <category
            id="org.eclipse.emf.ecore.Wizard.category.ID"
            name="%_UI_Wizard_category"/>
      <wizard
            id="us.coastalhacking.semiotics.model.control.presentation.ControlModelWizardID"
            name="%_UI_ControlModelWizard_label"
